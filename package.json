{
  "name": "charter-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve -p 9000",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "build": "rm -rf ./dist && ng build --prod --env=prod",
    "deploy": "npm run build && npm run firebase-deploy",
    "firebase-deploy": "firebase deploy --project charter-frontend",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@types/elasticsearch": "^5.0.12",
    "@types/headroom": "^0.7.30",
    "@types/html-entities": "^1.2.15",
    "@types/jquery": "^2.0.39",
    "@types/lodash": "^4.14.50",
    "@types/materialize-css": "^0.97.34",
    "angular2-materialize": "^6.6.0",
    "angularfire2": "^2.0.0-beta.7",
    "core-js": "^2.4.1",
    "elasticsearch": "^12.1.3",
    "firebase": "^3.6.6",
    "headroom.js": "^0.9.3",
    "html-entities": "^1.2.0",
    "jquery": "^2.2.4",
    "materialize-css": "^0.97.8",
    "markdown-to-html-pipe": "^1.2.0",
    "rxjs": "^5.0.1",
    "showdown": "^1.6.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.26",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
